{
  "name": "ng-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve sample-app -o",
    "build": "ng build --watch",
    "build:prod": "ng build --project=ng-blog",
    "single-test": "ng test --code-coverage --watch=false --project=ng-blog --browsers=ChromeHeadlessNoSandbox",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase-host-local": "ng build --project=sample-app --prod && firebase serve --only hosting",
    "firebase-deploy": "firebase deploy --only hosting:garrett-manley-blog"
  },
