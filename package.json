        "major": "npm version major && npm publish && git push",
        "test": "nodemon ./bin/index.js --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GarrettManley/ng-blog.git"
    },
    "keywords": [
        "Angular",
        "Blog",
        "Firebase"
    ],
    "author": "Garrett Manley <garrettmanley@gmail.com>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/GarrettManley/ng-blog/issues"
    },
    "homepage": "https://github.com/GarrettManley/ng-blog#readme",
    "dependencies": {
        "colors": "^1.3.3",
        "npm": "^6.10.0"
    },
    "devDependencies": {
        "@types/node": "^12.6.2",
        "cypress": "^3.4.0",
        "nodemon": "^1.19.1",
        "tsc": "^1.20150623.0",
        "typescript": "^3.5.3"
    }
}
