{
    "name": "ng-blog",
    "version": "1.1.0",
    "description": "Generates an Angular application with a blogging feature setup and ready to be deployed to firebase.",
    "main": "./bin/index.js",
    "bin": {
        "ng-blog": "./bin/index.js"
    },
    "scripts": {
        "build": "tsc -w --outDir ./out ./lib/index.ts",
        "patch": "npm version patch && npm publish && git push",
